{
  "name": "strength-ui",
  "version": "0.0.29",
  "description": "A React.js UI/Map components for Web",
  "repository": "https://",
  "homepage": "https://",
  "author": "wangxiping <wangxiping@.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "lib",
    "es",
    "dist",
    "static",
    "utils",
    "LICENSE"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "yarn dev",
    "clean": "rimraf dist lib es",
    "dev": "start-storybook -p 8080 -c .storybook",
    "upgrade": "yarn upgrade",
    "stylelint:fix": "stylelint 'components/**/*.less' --syntax less --fix",
    "eslint:fix": "eslint ./components --fix",
    "lint": "prettier --write \"components/**/*.tsx\" && yarn eslint --fix \"components/**/*.tsx\" && yarn stylelint:fix",
    "test": "cross-env BABEL_ENV=test jest --config jest.config.js --passWithNoTests",
    "test:update": "yarn test -u --passWithNoTests",
    "coverage": "yarn test --coverage",
    "ci:coverage": "yarn coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build:docs": "build-storybook -c .storybook -o .out",
    "pub:docs": "yarn build:docs && storybook-to-ghpages --existing-output-dir=.out",
    "prepublish": "yarn build",
    "build": "yarn run clean && yarn build:lib && yarn build:es && yarn build:umd && yarn build:css",
    "build:css": "cd scripts && gulp",
    "build:lib": "cross-env OUTPUT_MODULE=commonjs babel components -d lib --extensions '.ts,.tsx' --ignore **/__tests__",
    "build:es": "babel components -d es --extensions '.ts,.tsx' --ignore **/__tests__",
    "build:umd": "webpack --config ./scripts/build.umd.js",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "storybook": "build-storybook -c .storybook -o .out",
    "watch_es": "node ./watch_es.js",
    "tsc": "tsc",
    "tsc-w": "tsc -w"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.tsx": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "storybook-deployer": {
    "gitUsername": "The-End-Hero",
    "gitEmail": "527409987@qq.com",
    "commitMessage": "docs: deploy docs"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "sideEffects": [
    "dist/*",
    "lib/*",
    "es/*",
    "*.less"
  ],
  "commitizen": {
    "path": "cz-conventional-changelog"
  },
  "release": {
    "prepare": [
      {
        "path": "@semantic-release/changelog"
      },
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ]
    ]
  },
  "keywords": [
    "react",
    "tool",
    "react-component",
    "component",
    "components",
    "ui",
    "web",
    "map",
    "framework",
    "frontend"
  ],
  "tags": [
